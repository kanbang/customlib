	-$(DEL_FILE) -r $(INSTALL_ROOT)/opt/smarthome/qml/smarthome
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/smarthome/qml/ 


install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/smarthome/bin/ || $(MKDIR) $(INSTALL_ROOT)/opt/smarthome/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/opt/smarthome/bin/$(QMAKE_TARGET)"
	-$(STRIP) "$(INSTALL_ROOT)/opt/smarthome/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/smarthome/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/smarthome/bin/ 


